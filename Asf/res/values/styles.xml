<?xml version="1.0" encoding="UTF-8"?><resources>

    <style name="WidgetBackground">
        <item name="android:background">@drawable/widget_bg</item>
    </style>
    <style name="account_label">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">4dip</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:paddingRight">4dip</item>
        <item name="android:layout_marginTop">6dip</item>
        <item name="android:gravity">right</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#ff1c40a4</item>
    </style>

    <style name="account_field">
    	<item name="android:singleLine">true</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">56dip</item>
        <item name="android:textColor">#000000</item>
        <item name="android:background">@color/facebook_light_grey</item>
    </style>
    
    <!-- For cmcc push mail -->
    <style name="cmnew_setting_help">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:paddingBottom">4dip</item>
        <item name="android:layout_marginLeft">4dip</item>
        <item name="android:paddingRight">4dip</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">italic</item>
        <item name="android:textColor">#ff1c40a4</item>
    </style>  
    
    <!-- sns username style -->
    <style name="sns_username">
        <item name="android:textSize">14dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#333333</item>
    </style>   
    <style name="sns_username_blue">
        <item name="android:textSize">11dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#3B5998</item>
    </style>     
    <!-- sns username style -->
    <style name="sns_big_username">
        <item name="android:textSize">14dip</item>
